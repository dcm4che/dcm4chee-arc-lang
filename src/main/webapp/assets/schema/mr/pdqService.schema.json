{
  "title": "PDQ सेवा",
  "description": "PDQ सेवा वर्णनकर्ता.",
  "type": "object",
  "required": [
    "dcmPDQServiceID",
    "dcmURI",
    "dcmEntity"
  ],
  "properties": {
    "dcmPDQServiceID": {
      "title": "PDQ सेवा ID",
      "description": "PDQ सेवा ID.",
      "type": "string",
      "use": [
        "$.dcmDevice.dcmArchiveDevice.dcmPatientVerificationPDQServiceID"
      ]
    },
    "dcmURI": {
      "title": "PDQ सर्व्हिस URI",
      "description": "PDQ सर्व्हिस URI, उदाहरण 'pdq-dicom:FINDSCP' किंवा 'pdq-hl7:SendingApplication/SendingFacility:ReceivingApplication/ReceivingFacility'",
      "type": "string"
    },
    "dicomDescription": {
      "title": "PDQ सेवेचे वर्णन",
      "description": "PDQ सेवेचे अनियंत्रित मजकूर वर्णन.",
      "type": "string"
    },
    "dcmTag": {
      "title": "रुग्णाच्या रेकॉर्डचे गुणधर्म",
      "description": "रुग्णाच्या रेकॉर्डचे गुणधर्म - निर्दिष्ट नसल्यास सर्व उपलब्ध रुग्णाच्या रेकॉर्डचे गुणधर्मांची चौकशी केली जाईल.",
      "type": "array",
      "items": {
        "type": "string"
      },
      "format": "dcmTag"
    },
    "dcmEntity": {
      "title": "घटक प्राप्ति",
      "description": "C-FIND SCP वरून रुग्णाचे रेकॉर्ड किंवा रुग्णाच्या रेकॉर्डचे अभ्यासांची प्राप्ती सूचित करते. फक्त DICOM PDQ सेवेंसाठी प्रभावी ('PDQ सेवा URI': pdq-dicom:{AETitle})",
      "type": "string",
      "enum": [ "Patient", "Study" ],
      "default": "Patient"
    },
    "dcmProperty": {
      "title": "PDQ सेवा गुणधर्म",
      "description": "गुणधर्म या स्वरूपात <नाव> = <मूल्य>, उदाहरण 'LocalAET=DCM4CHEE' किंवा 'XSLStylesheetURI=${jboss.server.temp.url}/dcm4chee-arc/hl7-adt2dcm.xsl'",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
