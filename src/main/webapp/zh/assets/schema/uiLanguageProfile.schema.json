{
  "title": "UI Elasticsearch URL配置",
  "description": "UI Elasticsearch URL配置",
  "type": "object",
  "required": [
    "dcmuiLanguageProfileName",
    "dcmuiDefaultLanguage"
  ],
  "properties": {
    "dcmuiLanguageProfileName": {
      "title": "名称",
      "description": "语言配置文件名称",
      "type": "string"
    },
    "dcmDefaultLanguage": {
      "title": "默认语言",
      "description": "选择UI的默认语言。",
      "type": "string",
      "items" : {
        "type": "string"
      },
      "format": "dcmDefaultLanguage"
    },
    "dcmuiLanguageProfileRole": {
      "title": "角色",
      "description": "必须应用此语言配置文件的用户名角色。",
      "type": "array",
      "items" : {
        "type": "string"
      }
    },
    "dcmuiLanguageProfileUsername": {
      "title": "用户名",
      "description": "语言配置文件的用户名。",
      "type": "string"
    }
  }
}
