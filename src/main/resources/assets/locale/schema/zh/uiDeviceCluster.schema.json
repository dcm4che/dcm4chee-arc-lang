{
  "title": "UI集群配置",
  "description": "UI集群配置",
  "type": "object",
  "required": [
    "dcmuiDeviceClusterName",
    "dcmuiDeviceClusterDevices",
    "dcmuiDeviceClusterInstalled"
  ],
  "properties": {
    "dcmuiDeviceClusterName": {
      "title": "名称",
      "description": "集群名称",
      "type": "string"
    },
    "dcmuiDeviceClusterDescription": {
      "title": "描述",
      "description": "集群描述",
      "type": "string"
    },
    "dcmuiDeviceClusterDevices": {
      "title": "设备的Web应用程序",
      "description": "选择用于访问相应设备的预配置Web应用程序",
      "type": "array",
      "items" : { "type": "string" },
      "format": "webApp"
    },
    "dcmuiClusterWebApp": {
      "title": "Web应用程序",
      "description": "QIDO-RS类Web应用程序",
      "type":  "string",
      "format": "webApp"
    },
    "dcmuiDeviceClusterInstalled": {
      "title": "已安装",
      "description": "在UI中使用此URL",
      "type": "boolean",
      "default": true
    }
  }
}
