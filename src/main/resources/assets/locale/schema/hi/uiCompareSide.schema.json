{
  "title": "पक्षों की तुलना करें",
  "description": "पक्षों की तुलना करें ।",
  "type": "object",
  "required": [
    "dcmuiCompareSideName",
    "dcmuiCompareSideCluster",
    "dcmuiCompareSideElasticsearch",
    "dcmuiCompareSideInstalled",
    "dcmuiCompareSideOrder"
  ],
  "properties": {
    "dcmuiCompareSideName": {
      "title": "नाम",
      "description": "पक्षों की तुलना का नाम ।",
      "type": "string"
    },
    "dcmuiCompareSideDescription": {
      "title": "वर्णन",
      "description": "इस समूह का संक्षेप वर्णन ।",
      "type": "string"
    },
    "dcmuiCompareSideOrder": {
      "title": "क्रम",
      "description": "पक्षों की तुलना का क्रम ।",
      "type": "integer",
      "default":0,
      "enum":[
         0,
         1
      ]
    },
    "dcmuiCompareSideCluster": {
      "title": "क्लस्टर",
      "description": "क्लस्टर को चुने ।",
      "type": "string",
      "format":"dcmuiDeviceClusterObject"
    },
    "dcmuiCompareSideElasticsearch": {
      "title": "इलास्टिकसर्च",
      "description": "इलास्टिकसर्च URL को चुने ।",
      "type": "string",
      "format":"dcmuiElasticsearchConfig"
    },
    "dcmuiCompareSideQueueName": {
      "title": "कतार",
      "description": "पक्षों की तुलना के लिए इस्तमाल किए जानेवाले कतार का नाम ।",
      "type": "string",
      "format":"dcmQueueName"
    },
    "dcmuiCompareSideInstalled": {
      "title": "स्थापित",
      "description": "हाँ अगर यह पक्षों की तुलना डैशबोर्ड में सक्रीय है ।",
      "type": "boolean",
      "default": true
    }
  }
}
